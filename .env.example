# Prime Agent Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Claude API Key
ANTHROPIC_API_KEY=sk-ant-your-api-key-here

# Optional: Custom Anthropic API endpoint (leave empty for default)
ANTHROPIC_BASE_URL=

# Required: API authentication token (generate a secure random string)
AUTH_TOKEN=your-secure-random-token-here

# Optional: Git configuration (only needed if git.enabled=true in config)
# For SSH authentication:
GIT_SSH_KEY=

# For HTTPS authentication:
GIT_USERNAME=
GIT_TOKEN=

# Optional: Apple Push Notifications (only needed if apn.enabled=true in config)
APPLE_TEAM_ID=
APPLE_BUNDLE_ID=
APPLE_KEY_ID=
APPLE_P8_KEY=
